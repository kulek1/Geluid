{
  "name": "geluid-cra",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@suldashi/lame": "^1.2.5",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-select": "^3.0.0",
    "@types/react-transition-group": "^4.2.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "electron-is-dev": "^1.1.0",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "naudiodon": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4",
    "react-transition-group": "^4.2.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "roboto-fontface": "^0.10.0",
    "socket.io": "^2.2.0",
    "socket.io-stream": "^0.9.1"
  },
  "scripts": {
    "dev": "concurrently \"BROWSER=none yarn cra:start\" \"wait-on http://localhost:3000 && electron .\"",
    "cra:start": "rescripts start",
    "cra:build": "rescripts build",
    "cra:eject": "react-scripts eject",
    "test": "rescripts test",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -mw",
    "build-client": "cd client && npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.1",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0"
  },
  "author": {
    "name": "Geluid",
    "email": "todo@todo.com",
    "url": "https://github.com/kulek1/Geluid/"
  },
  "devEngines": {
    "node": ">=7.x <10.0.0",
    "npm": ">=5.x",
    "yarn": ">=1.0"
  },
  "build": {
    "appId": "com.mk.geluid",
    "productName": "Geluid",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  }
}
